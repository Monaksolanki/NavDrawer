<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.monika.navdrawer.HomeFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Welcome"
        android:id="@+id/textView4"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="#9aa738"
        android:textColorHighlight="#e47070"
        android:textSize="25dp"
        android:textStyle="bold"
        android:autoText="false"
        android:layout_marginTop="5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your Latest Parameters:"
        android:id="@+id/textView5"
        android:layout_below="@+id/textView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textIsSelectable="false"
        android:textSize="20dp"
        android:textColor="#da8e5c"
        android:textStyle="bold"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp" />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:outlineProvider="bounds"
        android:id="@+id/tableLayout">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:text="ECG:"
                android:id="@+id/textView6"
                android:layout_column="0"
                android:layout_margin="5dp"
                android:textSize="18dp"
                android:textColor="#6e1b5c"
                android:textStyle="bold"
                android:background="#d7d1d2"
                android:layout_marginLeft="15dp"
                android:layout_width="fill_parent"
                android:paddingStart="3dp"
                android:paddingLeft="3dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ecg"
                android:layout_column="2" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:text="Temperature:"
                android:id="@+id/textView7"
                android:layout_column="0"
                android:layout_margin="5dp"
                android:textSize="18dp"
                android:textColor="#6e1b5c"
                android:background="#d7d1d2"
                android:textStyle="bold"
                android:layout_marginLeft="15dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/temp"
                android:layout_column="2" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:text="Pulse Rate:"
                android:id="@+id/textView8"
                android:layout_column="0"
                android:background="#d7d1d2"
                android:layout_margin="5dp"
                android:textSize="18dp"
                android:textColor="#6e1b5c"
                android:textStyle="bold"
                android:paddingStart="3dp"
                android:paddingLeft="3dp"
                android:layout_marginLeft="15dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/pulse"
                android:layout_column="2" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:text="Symptoms: "
                android:id="@+id/ss"
                android:layout_column="0"
                android:layout_margin="5dp"
                android:textSize="18dp"
                android:textColor="#6e1b5c"
                android:textStyle="bold"
                android:background="#d7d1d2"
                android:layout_marginLeft="15dp"
                android:layout_width="fill_parent"
                android:paddingStart="3dp"
                android:paddingLeft="3dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/symp"
                android:layout_column="2" />

        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/tableLayout">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:text="Symptoms"
                android:layout_column="0"
                android:layout_margin="5dp"
                android:textSize="18dp"
                android:textColor="#6e1b5c"
                android:background="#d7d1d2"
                android:textStyle="bold"
                android:paddingStart="3dp"
                android:paddingLeft="3dp"
                android:layout_marginLeft="15dp"
                android:layout_span="2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView15"
                android:layout_column="2" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView16"
                android:layout_column="2" />
        </TableRow>
    </TableLayout>


</RelativeLayout>
